#:import CardWidgetTurnInProcess app.common.customWidgets.cardWidgetTurnInProcess
<CardWidgetTurnInProcess>:
    id:cardInTurn
    line_color:(0.2, 0.2, 0.2, 0.8)
    style:"elevated"
    name:''
    window:''
    size_hint_y:None
    BoxLayout:
        orientation:'horizontal'
        padding:dp(8)
        MDLabel:
            id:label_name
            halign: "center"
            text: root.name
            bold: True
            font_size:dp(24)
        MDIcon:
            icon: 'arrow-right-circle'
            font_size: dp(32)
        MDLabel:
            id: label_window
            text:root.window
            bold: True
            font_size:dp(24)
            halign: "center"
